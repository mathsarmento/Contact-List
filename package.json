{
  "name": "list-cont",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"npm run json:server\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "json:server": "json-server --watch src/db/db.json",
    "format:write": "prettier --config ./.prettierrc --write --list-different \"src/{app,enviroments}/**/*{.ts,.html,.scss}\"",
    "format:staged": "pretty-quick --staged --pattern \"src/{app,enviroments}/**/*{.ts,.html,.scss}\"",
    "lint:staged": "lint-staged",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/cdk": "^16.2.10",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/fire": "7",
    "@angular/forms": "^16.2.12",
    "@angular/material": "^16.2.12",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "firebase": "^10.7.2",
    "ngx-mask": "^16.4.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.2",
    "zone.js": "^0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.10",
    "@angular/cli": "~16.1.4",
    "@angular/compiler-cli": "^16.2.12",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jasmine-core": "~4.6.0",
    "json-server": "^0.17.4",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.1.0",
    "prettier": "3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "eslintIgnore": [
    "**/*.scss"
  ],
  "lint-staged": {
    "src/{app,enviroments}/**/*.{ts,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.19.1"
  }
}
